<script>
    $(function () {
        //post请求跟据barcode去查对一个的reportid去查result的结果数据并遍历到页面,
        let vals = {}
        vals.reportid = '<%=data.ReportID%>'
        vals.barcode = '<%=data.Barcode%>'
        $.post("admin/epihpv/inserval", vals, function (data) {
            console.info(data)
            hpvpositive="P"
            for (var i = 0; i < data.length; i++) {
                $("#hpvbox").append(
                    "<div style='width: 20%;height: 40px;float: left;line-height: 40px;background-color: #f6fdf8;'>" +
                    "<label style=height: 40px;line-height: 40px;'>" + data[i].HospRptItemName + "</label>: " +
                    "<text type='text' style='width: 55%;height: 30px;border: none;' >" + data[i].Result_Str + "</text> " +
                    "</div>")
               if(data[i].Result_Flag==hpvpositive){
                $("[id=result_analysis]").text(data[i].HospRptItemName+":"+data[i].Result_Str)
               }else{
                $("[id=result_analysis]").text("HPV"+data[0].Result_Ref)
               }
               
            }

        })
        //去查询数据库中的epihpvrep表，并把结果返回填充到页面
        $.post("admin/epihpv/inserinputval", vals, function (data) {
            $("[id=samplesta]").val(data.samplesta)
            $("[id=deliverdoc]").val(data.deliverdoc)
            $("[id=deliverdoctel]").val(data.deliverdoctel)
        })
    });


    //保存报告页面手动输入的几个数据
    $("input[id=save]").click(function () {
        $('input').attr('value', function () {
            return $(this).val();
        });
        var finalHtml = {}
        finalHtml.samplesta = $('#samplesta').val()
        finalHtml.deliverdoc = $('#deliverdoc').val()
        finalHtml.deliverdoctel = parseInt($('#deliverdoctel').val())
        finalHtml.barcode = '<%=data.Barcode%>'
        finalHtml.reportPage = "<html>" + $('html').html() + "</html>"
        console.info(finalHtml.barcode + finalHtml.deliverdoc + finalHtml.deliverdoctel)
        finalHtml.reportPage = "<html>" + $('html').html() + "</html>"
        $.post("/admin/saveHpvReport", finalHtml, function (data) {
            if (data == "success") alert("保存成功")

        })
    })
    //onblur()事件监听页面手动输入的几个值并获取数据，传到后台保存数据库，加载时再更新到报告页面值
    function updateval(obj) {
        let data = {}
        data.barcode = '<%=data.Barcode%>'
        data.col = $(obj).attr("name")
        data.val = $(obj).val()
        console.info(data)
        $.post("admin/epihpv/updateval", data, function (data) {
        })
    }


    $("input[id=generate_pdf]").click(function () {
        var pdf = {}
        pdf.barcode = '<%=data.Barcode%>'
        $.post("admin/epihpv/buildhpvpdf", pdf, function (data) {
            if (data == "success") alert("pdf generated success")
        })
    })
    // })
</script>

