                            html.push('<input type="hidden" name="id" value=' + jsondata[i]._id + '>')
                            html.push('<td>' + jsondata[i].createtime + '</td>')
                            html.push('<td>')
                            html.push('<button id="delete" type="button" onclick="confirm(this)">Delete</button>')
                            html.push('</td>')
                            html.push('</tr>')
                        }
                        html.push('</tbody>')
                        $("#tbody").replaceWith(html.join(""))

                    })
                }
            })
        })

